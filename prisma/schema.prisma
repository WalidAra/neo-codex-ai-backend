generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id       String   @id @default(cuid())
  email    String   @unique
  name     String
  image    String?
  password String
  provider Provider
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}

enum Provider {
  EMAIL
  GOOGLE
}